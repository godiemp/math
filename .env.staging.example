# Frontend Staging Environment Variables
# Configure these in Vercel project settings for staging environment

# Backend API URL - Points to staging backend
NEXT_PUBLIC_API_URL=https://paes-math-backend-staging.up.railway.app

# Environment Identifier
NEXT_PUBLIC_ENVIRONMENT=staging

# Monitoring & Analytics - Consider separate projects for staging
NEXT_PUBLIC_SENTRY_DSN=your-staging-sentry-dsn
NEXT_PUBLIC_POSTHOG_KEY=your-staging-posthog-key

# Feature Flags
NEXT_PUBLIC_ENABLE_EXPERIMENTAL_FEATURES=true

# Debug Mode
NEXT_PUBLIC_DEBUG_MODE=true
